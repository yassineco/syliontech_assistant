{
  "name": "@syliontech/server",
  "version": "1.0.0",
  "description": "SylionTech Assistant API Server - Fastify + Vertex AI Gemini",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@google-cloud/vertexai": "^1.7.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "firebase-admin": "^12.0.0",
    "pino": "^8.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/coverage-v8": "^1.1.0",
    "pino-pretty": "^13.1.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "fastify",
    "typescript",
    "vertex-ai",
    "gemini",
    "firebase",
    "sofinco"
  ]
}